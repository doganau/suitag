# Server Configuration
NODE_ENV=development
PORT=3001
HOST=localhost

# Database Configuration
DATABASE_URL="postgresql://username:password@localhost:5432/linktree_analytics?schema=public"

# Redis Configuration
REDIS_URL="redis://localhost:6379"
REDIS_PASSWORD=""

# Sui Network Configuration
SUI_NETWORK=testnet
SUI_RPC_URL=https://fullnode.testnet.sui.io:443
PACKAGE_ID=0xc5439e27cc0f5d4e63f410c8d3972bab712fdd2fd235540f9eb0148566dfc6fb
REGISTRY_ID=0xa619412963d391576358f631a35c7724c8614fd6ca596023f5c8461853f313e9

# Analytics Configuration
ANALYTICS_CACHE_TTL=3600
REALTIME_ENABLED=true
BATCH_SIZE=1000
CLEANUP_INTERVAL_HOURS=24

# Security
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
API_RATE_LIMIT=100
API_RATE_WINDOW=15

# External Services
GEOIP_ENABLED=true
USER_AGENT_PARSING=true

# Logging
LOG_LEVEL=info
LOG_FILE=logs/app.log

# CORS
CORS_ORIGIN=http://localhost:5173,https://outlierchain.trwal.app
CORS_CREDENTIALS=true

# WebSocket
WS_ENABLED=true
WS_HEARTBEAT_INTERVAL=30000

# Data Retention (days)
RETENTION_VIEWS=365
RETENTION_CLICKS=365
RETENTION_SESSIONS=90
RETENTION_CACHE=7

# zkLogin & Sponsored Transactions (Optional)
# Get your Enoki API key from: https://enoki.mystenlabs.com
ENOKI_API_KEY=your_enoki_api_key_here
